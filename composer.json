{
    "name": "goodm4ven/filament-map-tiler",
    "version": "1.1.120-dev",
    "description": "A Filament Forms field that provides an interactive MapTiler map for selecting and storing geographical coordinates.",
    "keywords": [
        "GoodM4ven",
        "laravel",
        "livewire",
        "tallstack",
        "filament",
        "map-tiler"
    ],
    "homepage": "https://github.com/GoodM4ven/filament-map-tiler",
    "support": {
        "issues": "https://github.com/GoodM4ven/filament-map-tiler/issues",
        "source": "https://github.com/GoodM4ven/filament-map-tiler"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "GoodM4ven",
            "email": "goodm4ven@proton.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "GoodMaven\\FilamentMapTiler\\": "src/",
            "GoodMaven\\FilamentMapTiler\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoodMaven\\FilamentMapTiler\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "phpstan": "vendor/bin/phpstan analyse --error-format=github --memory-limit=1G",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "lint": "vendor/bin/pint -v",
        "checks": [
            "@lint",
            "@phpstan",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GoodMaven\\FilamentMapTiler\\FilamentMapTilerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
